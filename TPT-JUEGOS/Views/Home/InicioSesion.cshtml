
@{
    ViewData["Title"] = "InicioSesion";
}
<!DOCTYPE html>
<html lang="es">
<head>
	<meta charset="utf-8">
	<title>Log In</title>
</head>

<body>

	<form action="ValidarLogin.aspx" method="post">
		<h1> Login </h1>

		<label for="miemail"> Mail: </label>
		<input type="email" id="miemail" name="correoelectronico" placeholder="Ingrese su email">
		<br><br>
		<label for="pass">Pass: </label>
		<input type="password" id="pass" name="password" placeholder="su clave" required>
		<br><br>
		<label for="verif">Acepta Terminos y Condiciones</label>
		<input type="checkbox" id="verif">
		<br><br>

		<input type="submit" value="ENVIAR">
	</form>

</body>

</html>

